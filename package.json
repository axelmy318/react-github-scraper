{
  "name": "react-github-scaper",
  "version": "0.0.1",
  "description": "Provides multiple customizable components to display data about a github repository using simply the repository name",
  "scripts": {
    "rollup": "rollup -c",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "publish_github": "npmrc axelmy_github && npm run rollup && npm publish",
    "publish_npmjs": "npmrc axelmy_npmjs && npm run rollup && npm publish --access=public --scope=unscoped"
  },
  "author": {
    "name": "Axel Milliery",
    "email": "a.milliery@icloud.com"
  },
  "license": "MIT",
  "devDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "@babel/core": "^7.16.10",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@storybook/addon-actions": "^6.4.14",
    "@storybook/addon-essentials": "^6.4.14",
    "@storybook/addon-links": "^6.4.14",
    "@storybook/react": "^6.4.14",
    "babel": "^6.23.0",
    "babel-loader": "^8.2.3",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "npmrc": "^1.1.1",
    "rollup": "^2.65.0",
